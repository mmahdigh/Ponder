{
  "name": "ponder-refactor",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently 'npm run devserver' 'npm run arlocal:all'",
    "start:some": "NODE_ENV=development concurrently 'npm run devserver' 'npm run arlocal:some'",
    "build": "NODE_ENV=production webpack",
    "devserver": "NODE_ENV=development webpack-dev-server",
    "arlocal:all": "concurrently 'arlocal' 'npm run seed:all'",
    "arlocal:some": "concurrently 'arlocal' 'npm run seed:some'",
    "seed:all": "node ./seeders/all-episodes",
    "seed:some": "node ./seeders/some-episodes",
    "test": "npm run lint && npm run jest && npm run cy:run",
    "lint": "eslint .",
    "jest": "NODE_ENV=test jest -i",
    "jest:watch": "npm run jest -- --watchAll",
    "cy:run": "NODE_ENV=test npm run cy -- run",
    "cy:open": "NODE_ENV=test npm run cy -- open",
    "cy": "NODE_OPTIONS=--openssl-legacy-provider cypress"
  },
